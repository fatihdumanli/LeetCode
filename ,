class Program {
	public static void main(String[] args) {
		var nums = new int[] { 2,3, -2,4 };
		var result = maxProduct(nums);
	}

	public static int maxProduct(int[] nums) {

		var min = 1;
		var max = 1;

		for(var i = 0; i < nums.length; i++) {
			min = Math.min(Math.min(min, min * nums[i]), max * nums[i]);
		}

		return 0;
	}
}
